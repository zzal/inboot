{
  "name": "@zzal/inboot",
  "version": "0.0.2-alpha.6",
  "description": "Bootstrap fullstack projects on using SST or Amplify",
  "scripts": {
    "build": "npm run prepublishOnly",
    "prepublishOnly": "run-s build:esm build:types",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "build:esm": "tsc --declaration false --module nodenext --target esnext --outDir dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Alain Duchesneau <alain.duchesneau@gmail.com>",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@commander-js/extra-typings": "12.1.0",
    "chalk": "5.3.0",
    "configstore": "7.0.0",
    "figlet": "1.8.0",
    "ini": "5.0.0",
    "inquirer": "12.0.0",
    "limax": "4.1.0",
    "ora": "8.1.0"
  },
  "devDependencies": {
    "@types/configstore": "6.0.2",
    "@types/figlet": "1.7.0",
    "@types/ini": "4.1.1",
    "npm-run-all": "4.1.5",
    "typescript": "5.6.3"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "inboot": "dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/index.d.ts"
      ]
    }
  }
}
